{
    "name": "kbirenheide/laravel-ai",
    "description": "A laravel framework for connecting to OpenAI and Ollama API, sent prompts, collect responses, create agents and agent networks, and put them to work.",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "^7.8"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Kbirenheide\\L3MA\\": "src/"
        }
    },
    "extra": {
        "laravel": {
          "providers": [
            "Kbirenheide\\L3MA\\Providers\\L3MAServiceProvider"
          ]
        }
    },      
    "scripts": {
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize",
            "php artisan vendor:publish kbirenheide/laravel-ai"
        ]
    },
    "authors": [
        {
            "name": "Koray Birenheide"
        }
    ]
}
